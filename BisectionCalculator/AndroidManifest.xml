<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zackehh.bisect"
    android:versionCode="3"
    android:versionName="1.02" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/myHolo" >
        <activity
            android:name="com.zackehh.bisect.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zackehh.bisect.About"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_about" >
        </activity>
        <activity
            android:name="com.zackehh.bisect.Usage"
            android:label="@string/title_activity_usage" >
        </activity>
    </application>

</manifest>
